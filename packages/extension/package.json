{
  "name": "remindoro",
  "version": "1.7.3",
  "description": "Better Reminders in browser",
  "scripts": {
    "test": "vitest",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev-server/ release/ dist/ extension/ *.zip *.crx",
    "lint:es": "eslint '*/**/*.{ts,tsx}'",
    "lint": "prettier -l **/src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "prettier --write **/src/**/*.{js,jsx,ts,tsx}",
    "typecheck": "node_modules/typescript/bin/tsc --noEmit",
    "start:firefox": "bun run scripts/bun/dev.ts --browser=firefox",
    "start:chrome": "bun run scripts/bun/dev.ts --browser=chrome",
    "build:firefox": "bun run scripts/bun/prod.ts --browser=firefox",
    "build:chrome": "bun run scripts/bun/prod.ts --browser=chrome",
    "show:linked": "ls -l node_modules | grep ^l"
  },
  "keywords": [
    "Email Reminders",
    "time tracker",
    "chrome",
    "extension",
    "react",
    "redux",
    "firefox",
    "reminders",
    "browser reminders"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/palerdot/remindoro.git"
  },
  "devDependencies": {
    "@types/bun": "^1.0.12",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^9.0.6",
    "@types/webextension-polyfill": "^0.8.1",
    "@typescript-eslint/parser": "^5.59.2",
    "chalk": "5.3.0",
    "chrome-launch": "^1.1.4",
    "colors": "^1.3.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-watch": "^7.0.0",
    "execa": "^8.0.1",
    "gzip-size": "^7.0.0",
    "happy-dom": "^2.47.0",
    "ora": "^8.0.1",
    "prettier": "^2.1.2",
    "pretty-bytes": "^6.1.1",
    "prop-types": "^15.6.2",
    "react-dev-utils": "^12.0.1",
    "strip-json-comments": "^3.1.1",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.6.1",
    "web-ext": "^7.6.2",
    "yargs": "^17.7.2",
    "zip-a-folder": "^1.1.0"
  },
  "dependencies": {
    "@date-io/dayjs": "^2.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.3",
    "@mui/utils": "^5.15.14",
    "@mui/x-date-pickers": "^6.3.0",
    "@reduxjs/toolkit": "^2.2.3",
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.1",
    "react-router-dom": "^5.2.0",
    "react-slite": "^0.2.5",
    "tinybase": "^4.3.18",
    "uuid": "^9.0.1",
    "webextension-polyfill": "^0.10.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
